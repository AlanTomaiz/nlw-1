{
  "name": "ecoleta",
  "version": "1.0.0",
  "main": "src/server.js",
  "scripts": {
    "dev": "ts-node-dev --transpile-only --ignore-watch node_modules src/server.ts",
    "knex:seeds": "knex --knexfile knexfile.ts seed:run",
    "knex:create": "knex --knexfile knexfile.ts migrate:make",
    "knex:run": "knex --knexfile knexfile.ts migrate:latest"
  },
  "dependencies": {
    "express": "^4.17.1",
    "knex": "^0.95.6",
    "sqlite3": "^5.0.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.12",
    "ts-node": "^10.0.0",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.3.2"
  }
}
